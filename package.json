{
  "name": "boilerplate-sveltekit",
  "version": "1.0.0",
  "description": "Boilerpalte for Sveltekit-Apps with Sveltekit-i18n, TailwindCSS, Typescript, ESLint, Prettier, Svelte-Check, Svelte-Preprocess, Sveltekit-I18n, Vite, Playwright and Vitest.",
  "author": "Daniel Gaiduk",
  "license": "MIT",
  "keywords": [
    "svelte",
    "sveltekit",
    "boilerplate",
    "tailwindcss",
    "typescript",
    "eslint",
    "prettier",
    "svelte-check",
    "svelte-preprocess",
    "sveltekit-i18n",
    "vite",
    "playwright",
    "vitest"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview --host",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "devDependencies": {
    "@playwright/test": "^1.31.2",
    "@sentry/node": "^7.41.0",
    "@sentry/svelte": "^7.41.0",
    "@storybook/addon-essentials": "^7.0.0-beta.62",
    "@storybook/addon-interactions": "^7.0.0-beta.62",
    "@storybook/addon-links": "^7.0.0-beta.62",
    "@storybook/blocks": "^7.0.0-beta.62",
    "@storybook/svelte": "^7.0.0-beta.62",
    "@storybook/sveltekit": "^7.0.0-beta.62",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@sveltejs/adapter-node": "^1.2.1",
    "@sveltejs/kit": "^1.10.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.9",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "autoprefixer": "^10.4.13",
    "daisyui": "^2.51.3",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.21",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.0-beta.62",
    "svelte": "^3.55.1",
    "svelte-check": "^3.0.4",
    "svelte-meta-tags": "^2.6.5",
    "svelte-preprocess": "^5.0.1",
    "sveltekit-i18n": "^2.2.2",
    "tailwindcss": "^3.2.7",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.29.2",
    "zod": "^3.21.3"
  },
  "type": "module"
}
