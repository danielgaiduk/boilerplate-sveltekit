{
	"name": "boilerplate",
	"version": "1.0.0",
	"description": "Boilerpalte for Sveltekit-Apps with Sveltekit-i18n, TailwindCSS, Typescript, ESLint, Prettier, Svelte-Check, Svelte-Preprocess, Sveltekit-I18n, Vite, Playwright, Vitest and Svelte-Kit-Adapter-Auto.",
	"author": "Daniel Gaiduk",
	"license": "MIT",
	"keywords": [
		"svelte",
		"sveltekit",
		"boilerplate",
		"tailwindcss",
		"typescript",
		"eslint",
		"prettier",
		"svelte-check",
		"svelte-preprocess",
		"sveltekit-i18n",
		"vite",
		"playwright",
		"vitest",
		"svelte-kit-adapter-auto"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@playwright/test": "^1.29.1",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/kit": "^1.0.1",
		"@tailwindcss/aspect-ratio": "^0.4.2",
		"@tailwindcss/forms": "^0.5.2",
		"@tailwindcss/line-clamp": "^0.4.2",
		"@tailwindcss/typography": "^0.5.3",
		"@typescript-eslint/eslint-plugin": "^5.47.1",
		"@typescript-eslint/parser": "^5.47.1",
		"autoprefixer": "^10.4.7",
		"eslint": "^8.30.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.14",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.1",
		"prettier-plugin-svelte": "^2.9.0",
		"svelte": "^3.55.0",
		"svelte-check": "^3.0.1",
		"svelte-preprocess": "^4.10.7",
		"sveltekit-i18n": "^2.2.2",
		"tailwindcss": "^3.1.5",
		"tslib": "^2.4.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.3",
		"vitest": "^0.26.2"
	},
	"type": "module"
}
